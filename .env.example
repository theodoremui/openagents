# OpenAgents Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# API Keys
# =============================================================================

# OpenAI API Key (Required for all agents)
OPENAI_API_KEY=your_openai_api_key_here

# Google Maps API Key (Required for geo and map agents)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Yelp API Key (Required for yelp agent)
YELP_API_KEY=your_yelp_api_key_here

# ElevenLabs API Key (Required for voice features)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# =============================================================================
# LiveKit Configuration (Required for real-time voice)
# =============================================================================

# LiveKit Server URL (WebSocket URL)
# Get from: https://cloud.livekit.io
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_WS_URL=wss://your-project.livekit.cloud

# LiveKit API Credentials
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
